{
  "name": "algorithemic_dev",
  "version": "1.5.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "ui": "pnpm --filter @repo/ui ui",
    "prepare": "husky"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.5.3",
    "turbo": "^2.5.3",
    "husky": "^9.1.7"
  },
  "packageManager": "pnpm@10.12.2",
  "engines": {
    "node": ">=20.17.0"
  }
}
