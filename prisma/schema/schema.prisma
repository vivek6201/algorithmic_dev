generator client {
  provider        = "prisma-client-js"
  output          = "../../src/generated/prisma"
  previewFeatures = ["prismaSchemaFolder"]
  binaryTargets = ["native", "debian-openssl-1.1.x", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
